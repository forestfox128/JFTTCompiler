DECLARE
    n; m;
IN
    n := 3;
    m := 0;
    
    WHILE n >= m DO
        WHILE m = 0 DO
            WRITE 0;
            m := 1;
        ENDWHILE    
        WRITE n;
        WHILE n != 5 DO
            WRITE 1;
            n := 5;
        ENDWHILE
        WRITE n;
        m := 4;
    ENDWHILE
    WRITE 0;

END